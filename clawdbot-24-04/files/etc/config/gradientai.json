{
  "models": {
    "mode": "merge",
    "providers": {
      "gradient": {
        "baseUrl": "https://inference.do-ai.run/v1",
        "apiKey": "PLACEHOLDER",
        "api": "openai-completions",
        "models": [
          {
            "id": "llama3.3-70b-instruct",
            "name": "Llama 3.3 70B Instruct",
            "reasoning": false,
            "input": [
              "text"
            ],
            "contextWindow": 128000,
            "maxTokens": 8192,
            "compat": {
              "supportsStore": false,
              "maxTokensField": "max_tokens"
            },
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            }
          },
          {
            "id": "deepseek-r1-distill-llama-70b",
            "name": "DeepSeek R1 Distill Llama 70B",
            "reasoning": true,
            "input": [
              "text"
            ],
            "contextWindow": 128000,
            "maxTokens": 8192,
            "compat": {
              "supportsStore": false,
              "maxTokensField": "max_tokens"
            },
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            }
          },
          {
            "id": "openai-gpt-oss-120b",
            "name": "OpenAI GPT OSS 120B",
            "reasoning": false,
            "input": [
              "text"
            ],
            "contextWindow": 128000,
            "maxTokens": 8192,
            "compat": {
              "supportsStore": false,
              "maxTokensField": "max_tokens"
            },
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            }
          },
          {
            "id": "anthropic-claude-4.5-sonnet",
            "name": "Claude 4.5 Sonnet",
            "reasoning": false,
            "input": [
              "text"
            ],
            "contextWindow": 200000,
            "maxTokens": 64000,
            "compat": {
              "supportsStore": false,
              "maxTokensField": "max_tokens"
            },
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            }
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "gradient/anthropic-claude-4.5-sonnet"
      },
      "models": {
        "gradient/llama3.3-70b-instruct": {
          "params": {
            "maxTokens": 8192
          }
        },
        "gradient/deepseek-r1-distill-llama-70b": {
          "params": {
            "maxTokens": 8192
          }
        },
        "gradient/openai-gpt-oss-120b": {
          "params": {
            "maxTokens": 8192
          }
        },
        "gradient/anthropic-claude-4.5-sonnet": {
          "params": {
            "maxTokens": 64000
          }
        }
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      },
      "sandbox": {
        "workspaceAccess":"rw",
        "mode": "all",
        "docker": {
          "network": "bridge",
          "binds": [
            "/home/openclaw/homebrew:/home/openclaw/homebrew:ro",
            "/opt/openclaw:/opt/openclaw:ro",
            "/usr/lib/node_modules/openclaw/skills:/home/openclaw/.openclaw/workspace/skills:ro"
          ]
        }
      }
    }
  },
  "gateway": {
    "mode": "local",
    "bind": "loopback",
    "auth": {
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    },
    "trustedProxies": ["127.0.0.1"]
  }
}